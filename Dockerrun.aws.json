{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name": "client",
            "image": "emilystacyandroid/multi-client",
            "hostname": "client",
            "essential": false

        },
        {
            "name": "server",
            "image": "emilystacyandroid/multi-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "worker",
            "image": "emilystacyandroid/multi-worker",
            "hostname": "worker",
            "essential": false 
        },
        {
            "name":"nginx",
            "image": "emilystacyandroid/multi-nginx",
            "essential":true,
            "portMappings":[

                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":["client", "server"]
        }
    ]
}